		"@babel/runtime": "^7.7.6",
		"@types/fs-extra": "8.0.1",
		"@types/jest": "^24.0.23",
		"@types/node": "^12.12.17",
		"@types/uuid": "^3.4.6",
		"babel-minify": "^0.5.1",
		"babel-preset-minify": "^0.5.1",
