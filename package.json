		"util.pkgwrap": "^0.0.115",
		"uuid": "^3.3.3"
	},
	"scripts": {
		"all": "yarn install && yarn build && yarn test && yarn manifest",
		"build": "yarn clean && yarn lint && yarn prettier && yarn build:es && yarn build:umd",
