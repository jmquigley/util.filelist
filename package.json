	"main": "./index.umd.min.js",
	"module": "./index.es.min.js",
	"types": "./index.d.ts",
	"files": [
		"index.*.min.js",
		"index.d.ts"
	],
	"dependencies": {
		"fs-extra": "^9.0.0",
		"util.rstrip": "0.0.58"
